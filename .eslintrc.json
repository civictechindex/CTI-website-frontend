{
  "env": {
      "browser": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 7,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "max-len": ["error", 
      { "code": 81 }
    ]
  }
}
